<div id="<%= dom_id profile %>">
  <p>
    <strong>Username:</strong>
    <%= profile.username %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= profile.name %>
  </p>

  <p>
    <strong>Website:</strong>
    <%= profile.website %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= profile.bio %>
  </p>

  <p>
    <strong>Account:</strong>
    <%= profile.account_id %>
  </p>

</div>
